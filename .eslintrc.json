{
    "extends": ["plugin:react/recommended", "prettier"],
    "plugins": ["promise", "react", "jest"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "shared-node-browser": true,
      "worker": true,
      "jquery": true,
      "serviceworker": true,
      "jest/globals": true
    },
    "globals": {
      "require": false,
      "exports": false,
      "module": false,
      "process": false,
      "global": false,
      "console": false,
      "Promise": false,
      "__dirname": false
    },
    "rules": {
      "react/no-unescaped-entities": "off",
      "react/prop-types": "off",
      "comma-dangle": [2, "always-multiline"],
      "no-cond-assign": [2, "except-parens"],
      "no-console": [2, { "allow": ["warn", "error"] }],
      "no-constant-condition": 2,
      "no-control-regex": 2,
      "no-debugger": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty-character-class": 2,
      "no-empty": 1,
      "no-ex-assign": 2,
      "no-extra-boolean-cast": 2,
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-inner-declarations": [2, "both"],
      "no-invalid-regexp": 2,
      "no-irregular-whitespace": 2,
      "no-negated-in-lhs": 2,
      "no-obj-calls": 2,
      "no-sparse-arrays": 2,
      "no-unreachable": 2,
      "use-isnan": 2,
      "valid-typeof": 2,
      "no-unexpected-multiline": 2,
      "block-scoped-var": 2,
      "curly": [2, "multi-line", "consistent"],
      "default-case": 2,
      "dot-notation": [2, { "allowKeywords": true }],
      "dot-location": [2, "property"],
      "eqeqeq": [2, "smart"],
      "no-alert": 1,
      "no-caller": 2,
      "no-div-regex": 2,
      "no-eq-null": 2,
      "no-eval": 2,
      "no-extend-native": 2,
      "no-extra-bind": 2,
      "no-fallthrough": 1,
      "no-floating-decimal": 2,
      "no-implicit-coercion": [
        2,
        { "boolean": true, "number": true, "string": true }
      ],
      "no-implied-eval": 2,
      "no-iterator": 2,
      "no-labels": 2,
      "no-lone-blocks": 2,
      "no-multi-spaces": 2,
      "no-native-reassign": 2,
      "no-new-func": 2,
      "no-new-wrappers": 2,
      "no-octal-escape": 2,
      "no-octal": 2,
      "no-proto": 1,
      "no-redeclare": [2, { "builtinGlobals": true }],
      "no-return-assign": [2, "always"],
      "no-script-url": 1,
      "no-self-compare": 2,
      "no-sequences": 2,
      "no-throw-literal": 2,
      "no-unused-expressions": 2,
      "no-useless-call": 2,
      "no-warning-comments": 1,
      "no-with": 2,
      "wrap-iife": [2, "any"],
      "no-catch-shadow": 2,
      "no-delete-var": 2,
      "no-label-var": 2,
      "no-shadow-restricted-names": 2,
      "no-undef-init": 2,
      "no-undef": 2,
      "no-unused-vars": [2, { "args": "none" }],
      "no-use-before-define": [2, "nofunc"],
      "callback-return": 2,
      "handle-callback-err": 2,
      "no-mixed-requires": 2,
      "no-new-require": 2,
      "no-process-exit": 1,
      "brace-style": [2, "1tbs", { "allowSingleLine": true }],
      "comma-spacing": [2, { "after": true }],
      "comma-style": 2,
      "consistent-this": 2,
      "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
      "new-parens": 2,
      "no-lonely-if": 2,
      "no-mixed-spaces-and-tabs": 2,
      "no-multiple-empty-lines": 2,
      "no-nested-ternary": 2,
      "no-trailing-spaces": [2, { "skipBlankLines": true }],
      "no-unneeded-ternary": 2,
      "quotes": [2, "single", "avoid-escape"],
      "semi": [2, "never"],
      "space-before-blocks": [2, "always"],
      "space-before-function-paren": [2, "never"],
      "space-infix-ops": 2,
      "spaced-comment": [2, "always", {}],
      "arrow-spacing": [2, { "before": true, "after": true }],
      "constructor-super": 2,
      "no-const-assign": 2,
      "no-this-before-super": 2,
      "prefer-const": 1,
      "prefer-spread": 1,
      "require-yield": 1
    }
  }